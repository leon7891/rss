<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:id="@+id/myLayout"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="local.leon.a2804.SelectedFeeds"
    tools:showIn="@layout/activity_selected_feeds">

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Get disabled\nitems"
        android:id="@+id/switchButton"
        android:layout_alignParentTop="true" />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/scrollView"
        android:layout_below="@+id/switchButton">
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@+id/numberText"
            android:layout_toStartOf="@+id/addRemoveButton"
            android:id="@+id/myLinearLayout">


        </LinearLayout>
    </ScrollView>
</RelativeLayout>
